{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@/*": ["*"]
    },
    "types": ["@nuxt/ui"],
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules"
  ]
}
